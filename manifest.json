{
  "name": "Tobarchive",
  "description": "Tobarchive",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "downloads",
    "activeTab",
    "scripting"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Download"
  }
}